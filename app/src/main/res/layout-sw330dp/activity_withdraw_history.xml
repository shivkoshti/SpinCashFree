<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gamebackground"
    tools:context=".WithdrawHistory">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/head"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/gametext"
            android:gravity="center_horizontal"
            android:padding="5dp"
            android:shadowColor="@color/darkgreen"
            android:text="Withdraw History"
            android:textColor="@color/orange"
            android:textSize="23dp" />

        <com.webknight.spinncash.extra.ExpandableHeightGridView
            android:id="@+id/expandableListView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/head"
            android:indicatorLeft="?android:attr/expandableListPreferredItemIndicatorLeft"
            android:numColumns="1"
            android:padding="5dp"
            android:scrollbars="none"
            android:verticalSpacing="2dp" />

        <ImageView
            android:id="@+id/nodata"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/head"
            android:src="@drawable/nodata"
            android:visibility="gone" />
    </RelativeLayout>
</ScrollView>